<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zrt.sec.mapper.sysAppUpgrade.SysAppUpgradeCustomMapper">
	<select id="getMaxVer" resultType="java.util.HashMap">
		select max(version_code) maxVersion from sys_app_upgrade sau where
		 sau.device_type=#{device_type}
	</select>
</mapper>